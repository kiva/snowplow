{
	"$schema": "https://raw.githubusercontent.com/kiva/snowplow/master/conf/snowplow_loan_display_event_schema_1_0_0.json#",
	"description": "Schema for Kiva loan display events",
	"self": {
		"vendor": "org.kiva",
		"name": "loan_display_event",
		"format": "jsonschema",
		"version": "1-0-0"
	},

	"type": "object",
	"properties": {
		"sectionIdentifier": {
			"type": "string"
		},
		"sectionPosition": {
			"type": "number"
		},
		"loansDisplayed": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"position": {
						"type": "number"
					},
					"loanId": {
						"type": "number"
					}
				}
			}
		}
	},
	"additionalProperties": false
}
