{
	"$schema": "http://iglucentral.com/schemas/com.snowplowanalytics.self-desc/schema/jsonschema/1-0-0#",
	"description": "Schema for Monthly Good checkout event",
	"self": {
		"vendor": "org.kiva",
		"name": "monthlygood_checkout_event",
		"format": "jsonschema",
		"version": "1-0-1"
	},

	"type": "object",
	"properties": {
		"id": {
			"type": "integer",
			"description": "The auto deposit id"
		},
		"subscriptionType": {
			"type": "string",
			"description": "Subscription type: monthly, one-time"
		},
		"category": {
			"type": "string",
			"description": "Monthly Good category"
		},
		"depositAmount": {
			"type": "number",
			"description": "Monthly deposit amount (auto-deposit amount)"
		},
		"donationAmount": {
			"type": "number",
			"description": "Monthly donation amount"
		},
		"paymentType": {
			"type": "string",
			"description": "Subscription payment type"
		}
	},
	"additionalProperties": false
}
