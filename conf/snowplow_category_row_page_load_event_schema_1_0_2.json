{
	"$schema": "https://raw.githubusercontent.com/kiva/snowplow/master/conf/snowplow_category_row_page_load_event_schema_1_0_2.json#",
	"description": "Schema for a Kiva /lend-by-category page load event",
	"self": {
		"vendor": "org.kiva",
		"name": "lend_by_category_page_load_event",
		"format": "jsonschema",
		"version": "1-0-2"
	},

	"type": "object",
	"properties": {
		"categorySetIdentifier": {
			"type": "string"
		},
		"categoriesDisplayed": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"sortsDisplayed": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"loansDisplayed": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"categoryId": {
						"type": "number"
					},
					"row": {
						"type": "number"
					},
					"position": {
						"type": "number"
					},
					"loanId": {
						"type": "number"
					}
				}
			}
		}
	},
	"additionalProperties": false
}
