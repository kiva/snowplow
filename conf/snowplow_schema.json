{
	"$schema": "https://raw.githubusercontent.com/kiva/vertex/master/conf/snowplow_schema.json#",
	"description": "Schema for a Kiva event",
	"self": {
		"vendor": "org.kiva",
		"name": "kiva_event",
		"format": "jsonschema",
		"version": "1-0-0"
	},

	"type": "object",
	"properties": {
		"source": {
			"type": "string"
		},
		"medium": {
			"type": "string"
		},
		"campaign": {
			"type": "string"
		},
		"campaignContent": {
			"type": "string"
		},
		"creditChangeId": {
			"type": "integer",
			"minimum": 0
		}
	},
	"additionalProperties": false
}